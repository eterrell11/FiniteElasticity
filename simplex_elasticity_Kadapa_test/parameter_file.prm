# Parameter file for three-field problem

subsection Material properties
  set Young's modulus = 10
  set Poisson's ratio = 0.4
  set Density = 1.0
end
subsection Time
  set Timestep =  0.001
  set End time =  0.1
  set Save time = 0.001
  set Start time = 0.0
end
# Time integrator options: 
# 1 for forward Euler
# 2 for SSPRK2
# 3 for SSPRK3
# 4 for modified trapezoidal rule
subsection Numerical parameters
  set alpha = 1.0
  set beta  = 1.0
  set Time integrator = 0
  set n_ref = 3
  set Velocity order = 2
  set Pressure order = 1
  set LumpMass = true
end
subsection Simulation parameters
  set Body force = 0.1
  set Initial velocity = 0.0
  set Traction Magnitude = 0.0
end



